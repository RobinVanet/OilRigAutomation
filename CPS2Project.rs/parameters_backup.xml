<?xml version="1.0" encoding="UTF-8" ?>
<parameters>
					<parameter name="surfaceTemp" displayName="Surface Temperature (in degree Celsius)" type="float" 
						defaultValue="20.0" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$FloatConverter"
									
		/>
				<parameter name="initialRPM" displayName="Initial RPM" type="double" 
						defaultValue="100.0" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"
									
		/>
				<parameter name="nbSensor" displayName="Number of Sensor Agents" type="int" 
						defaultValue="5" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
				<parameter name="drillingAngle" displayName="Drilling Angle" type="float" 
						defaultValue="30.0" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$FloatConverter"
									
		/>
				<parameter name="holeDiameter" displayName="Hole Diameter (inch)" type="float" 
						defaultValue="6.0" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$FloatConverter"
									
		/>
				<parameter name="nbActuator" displayName="Number of Uphole Actuator Agents" type="int" 
						defaultValue="1" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
				<parameter name="downHoleRPMPercentage" displayName="DownHole RPM Percentage (0-100)" type="float" 
						defaultValue="50.0" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$FloatConverter"
									
		/>
				<parameter name="startingRunDepth" displayName="Starting Run Depth" type="double" 
						defaultValue="4500.0" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"
									
		/>
				<parameter name="votingEnabled" displayName="Voting Enabled" type="boolean" 
						defaultValue="false" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$BooleanConverter"
									
		/>
				<parameter name="weightOnBit" displayName="WOB (0 to 30 klbf)" type="double" 
						defaultValue="1.0" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"
									
		/>
				<parameter name="depthGoal" displayName="Total Depth (in m)" type="int" 
						defaultValue="5000" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
				<parameter name="randomSeed" displayName="Default Random Seed" type="int" 
						defaultValue="__NULL__"
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
				<parameter name="constantWOBincrease" displayName="WOB constant increase" type="boolean" 
						defaultValue="true" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$BooleanConverter"
									
		/>
				<parameter name="increasePerTick" displayName="WOB Increase per Tick (in klbf)" type="double" 
						defaultValue="0.05" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"
									
		/>
				<parameter name="coolingEnabled" displayName="Cooling Enabled" type="boolean" 
						defaultValue="true" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$BooleanConverter"
									
		/>
				<parameter name="geothermalGradient" displayName="Geothermal Gradient (in Celsius/meter)" type="float" 
						defaultValue="0.03" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$FloatConverter"
									
		/>
				<parameter name="hardFormation" displayName="Hard Formation" type="boolean" 
						defaultValue="true" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$BooleanConverter"
									
		/>
		</parameters>


